    },
    "slug": {
      "type": "string"
    },
    "sections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "sections.hero",
        "sections.text-block",
        "sections.image-gallery",
        "sections.project-showcase",
        "sections.contact",
        "sections.info",
        "sections.fa-qs",
        "sections.experiences",
        "sections.skills",
        "sections.services"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    }
  }
}
