{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": false
    },
    "slug": {
      "type": "string"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured": {
      "type": "boolean"
    },
    "year": {
      "type": "date"
    },
    "client": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::client.client"
    },
    "link_demo": {
      "type": "string"
    },
    "link_github": {
      "type": "string"
    },
    "case_study": {
      "type": "component",
      "component": "sections.case-study",
      "repeatable": false
    },
    "description": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
